{
  "functions": {
    "api/discord-auth.js": {
      "runtime": "nodejs18.x"
    }
  },
  "buildCommand": "echo 'No build needed for static site'",
  "outputDirectory": ".",
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/dashboard",
      "destination": "/static-dashboard.html"
    },
    {
      "source": "/homepage",
      "destination": "/homepage.html"
    }
  ]
}
